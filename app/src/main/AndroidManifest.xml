<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.geequizsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".QuizSessionActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".StartActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CategorySelectActivity"
            android:label="@string/title_activity_categories"></activity>
        <activity
            android:name=".DashboardActivity"
            android:label="@string/title_activity_dashboard"></activity>
        <activity
            android:name=".ExplanationActivity"
            android:label="@string/title_activity_explanation"></activity>
    </application>

</manifest>